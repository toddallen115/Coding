{
  "env": {
    "node": true,
    "browser": true,
    "jasmine": true
  },
  "rules": {
    "indent": ["error", 2],
    "no-extra-parens": [2, "all"],
    "no-eq-null": 2,
    "no-self-compare": 2,
    "wrap-iife": 2,
    "brace-style": [2, "1tbs"],
    "comma-style": [2, "last"],
    "no-extra-semi": [1],
    "max-len": [1, 100, 4]
  }
}
